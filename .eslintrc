{
  "extends": [
    "react-app",
    "eslint:recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "rules": {
    "array-bracket-newline": [ "warn", "consistent" ],
    "array-bracket-spacing": [ "warn", "always" ],
    "arrow-body-style": [ "warn", "as-needed" ],
    "arrow-parens": [ "warn", "as-needed" ],
    "arrow-spacing": [ "warn", { "before": true, "after": true } ],

    "block-spacing": [ "warn", "always" ],
    "brace-style": [ "warn", "1tbs", { "allowSingleLine": true } ],

    "camelcase": [ "warn", { "properties": "never" } ],
    "comma-dangle": [ "warn", "never" ],
    "comma-spacing": [ "warn", { "before": false, "after": true } ],
    "comma-style": [ "warn", "last" ],
    "computed-property-spacing": [ "warn", "never" ],
    "consistent-return": "error",
    "consistent-this": [ "error", "self" ],
    "constructor-super": "error",
    "curly": [ "warn", "all" ],

    "dot-location": [ "warn", "property" ],
    "dot-notation": [ "warn", { "allowKeywords": true } ],

    "eol-last": "warn",
    "eqeqeq": [ "warn", "smart" ],

    "for-direction": "error",
    "func-call-spacing": "warn",
    "func-names": "warn",
    "function-paren-newline": [ "warn", "consistent" ],

    "generator-star-spacing": [ "warn", "after" ],
    "grouped-accessor-pairs": "warn",

    "handle-callback-err": "warn",

    "import/default": "warn",
    "import/export": "warn",
    "import/extensions": [ "warn", {
      "js": "never",
      "json": "always",
      "svg": "always",
      "png": "always",
      "jpg": "always",
      "ico": "always",
      "css": "always",
      "scss": "always"
    } ],
    "import/named": "warn",
    "import/namespace": "warn",
    "import/newline-after-import": "warn",
    "import/no-absolute-path": "warn",
    "import/no-cycle": "warn",
    "import/no-deprecated": "warn",
    "import/no-duplicates": "warn",
    "import/no-extraneous-dependencies": "warn",
    "import/no-mutable-exports": "warn",
    "import/no-named-as-default": "warn",
    "import/no-named-as-default-member": "warn",
    "import/no-named-default": "warn",
    "import/no-self-import": "warn",
    "import/no-unresolved": "warn",
    "import/no-unused-modules": "warn",
    "import/no-useless-path-segments": "warn",
    "import/order": [ "warn", {
      "groups": [ "builtin", "external", "internal", "parent", "sibling" ],
      "newlines-between": "always"
    } ],
    "indent": [ "warn", 2, {
      "SwitchCase": 1,
      "VariableDeclarator": 1,
      "outerIIFEBody": 1,
      "MemberExpression": 1,
      "FunctionDeclaration": { "parameters": 1, "body": 1 },
      "FunctionExpression": { "parameters": 1, "body": 1 },
      "CallExpression": {"arguments": 1},
      "ArrayExpression": 1,
      "ObjectExpression": 1,
      "ImportDeclaration": 1,
      "flatTernaryExpressions": false,
      "ignoredNodes": [ "JSXElement *", "JSXElement", "JSXAttribute", "JSXSpreadAttribute" ]
    } ],

    "jsx-quotes": [ "warn", "prefer-double" ],

    "key-spacing": [ "warn", { "beforeColon": false, "afterColon": true } ],
    "keyword-spacing": [ "warn", { "before": true, "after": true } ],

    "lines-around-comment": [ "warn", { "beforeBlockComment": true } ],

    "max-depth": [ 0, 3 ],
    "max-len": [ "warn", { "code": 121, "tabWidth": 2, "ignoreUrls": true } ],
    "max-nested-callbacks": [ 0, 3 ],
    "max-statements-per-line": [ "warn", { "max": 2 } ],
    "multiline-ternary": [ "warn", "always-multiline" ],

    "newline-before-return": "warn",
    "newline-per-chained-call": [ "warn", { "ignoreChainWithDepth": 3 } ],
    "no-alert": "warn",
    "no-array-constructor": "error",
    "no-async-promise-executor": "error",
    "no-bitwise": "error",
    "no-case-declarations": "warn",
    "no-class-assign": "warn",
    "no-compare-neg-zero": "warn",
    "no-confusing-arrow": "warn",
    "no-console": "off",
    "no-constant-condition": "warn",
    "no-continue": "warn",
    "no-debugger": "warn",
    "no-div-regex": "warn",
    "no-dupe-else-if": "error",
    "no-duplicate-imports": "warn",
    "no-else-return": "warn",
    "no-empty": "warn",
    "no-empty-function": [ "warn", { "allow": [ "arrowFunctions" ] } ],
    "no-ex-assign": "warn",
    "no-extra-boolean-cast": "warn",
    "no-extra-semi": "warn",
    "no-floating-decimal": "warn",
    "no-global-assign": "error",
    "no-inner-declarations": [ "warn", "both" ],
    "no-irregular-whitespace": "warn",
    "no-lonely-if": "warn",
    "no-mixed-requires": "error",
    "no-mixed-spaces-and-tabs": "warn",
    "no-multi-assign": "warn",
    "no-multi-spaces": "warn",
    "no-multiple-empty-lines": [ "warn", { "max": 2 } ],
    "no-nested-ternary": "error",
    "no-new": "error",
    "no-new-require": "error",
    "no-path-concat": "warn",
    "no-plusplus": [ "warn", { "allowForLoopAfterthoughts": true } ],
    "no-process-exit": "warn",
    "no-proto": "error",
    "no-prototype-builtins": "error",
    "no-restricted-modules": [ "warn", "" ],
    "no-return-assign": "warn",
    "no-return-await": "warn",
    "no-setter-return": "error",
    "no-shadow": "warn",
    "no-spaced-func": "warn",
    "no-sync": "warn",
    "no-trailing-spaces": "warn",
    "no-undef-init": "warn",
    "no-unexpected-multiline": "warn",
    "no-unneeded-ternary": "warn",
    "no-unsafe-finally": "warn",
    "no-unsafe-negation": "warn",
    "no-unused-vars": [ "warn", { "vars": "all", "args": "after-used" } ],
    "no-var": "error",

    "object-curly-spacing": [ "warn", "always" ],
    "object-shorthand": "warn",
    "one-var": [ "warn", "never" ],
    "operator-assignment": [ "warn", "never" ],
    "operator-linebreak": [ "warn", "before" ],

    "prefer-arrow-callback": "warn",
    "prefer-const": "warn",
    "prefer-numeric-literals": "warn",
    "prefer-object-spread": "warn",
    "prefer-promise-reject-errors": [ "warn", { "allowEmptyReject": true } ],
    "prefer-rest-params": "warn",
    "prefer-spread": "warn",
    "prefer-template": "warn",

    "quotes": [ "warn", "single" ],

    "react/button-has-type": "warn",
    "react/jsx-child-element-spacing": "warn",
    "react/jsx-closing-bracket-location": "warn",
    "react/jsx-closing-tag-location": "warn",
    "react/jsx-curly-brace-presence": [ "warn", "never" ],
    // "react/jsx-curly-newline": [ "warn", "consistent" ],
    "react/jsx-curly-spacing": [ "warn", "always", { "spacing": { "objectLiterals": "never" } } ],
    "react/jsx-equals-spacing": [ "warn", "never" ],
    "react/jsx-filename-extension": [ "error", { "extensions": [ ".js" ] } ],
    "react/jsx-first-prop-new-line": [ "warn", "multiline" ],
    "react/jsx-indent": [ "warn", 2, { "checkAttributes": true, "indentLogicalExpressions": true } ],
    "react/jsx-indent-props": [ "warn", 2 ],
    "react/jsx-key": "warn",
    "react/jsx-max-props-per-line": [ "warn", { "maximum": 1, "when": "multiline" } ],
    "react/jsx-no-comment-textnodes": "warn",
    "react/jsx-no-duplicate-props": "error",
    "react/jsx-no-script-url": "warn",
    "react/jsx-no-target-blank": "warn",
    "react/jsx-no-undef": "warn",
    "react/jsx-no-useless-fragment": "warn",
    "react/jsx-one-expression-per-line": [ "warn", { "allow": "single-child" } ],
    "react/jsx-pascal-case": "warn",
    "react/jsx-props-no-multi-spaces": "warn",
    "react/jsx-tag-spacing": [ "warn", { "beforeClosing": "never" } ],
    "react/jsx-wrap-multilines": [ "warn", {
      "declaration": "parens-new-line",
      "assignment": "parens-new-line",
      "return": "parens-new-line",
      "arrow": "parens-new-line",
      "condition": "parens-new-line",
      "logical": "parens-new-line",
      "prop": "ignore"
    } ],
    "react/no-array-index-key": "warn",
    "react/no-deprecated": "error",
    "react/no-unknown-property": "warn",
    "react/no-unused-state": "warn",
    "react/self-closing-comp": "warn",
    "react/void-dom-elements-no-children": "warn",
    "react-hooks/exhaustive-deps": "off",
    "require-atomic-updates": "warn",

    "semi": [ "warn", "always" ],
    "semi-spacing": [ "warn", { "before": false, "after": true } ],
    "semi-style": [ "warn", "last" ],
    "space-before-blocks": [ "warn", "always" ],
    "space-before-function-paren": [ "warn", "never" ],
    "space-in-parens": [ "warn", "never" ],
    "space-infix-ops": "warn",
    "spaced-comment": [ "warn", "always" ],

    "template-curly-spacing": "warn",

    "vars-on-top": "warn",

    "wrap-iife": [ "warn", "inside" ],
    "wrap-regex": "warn",

    "yoda": [ "warn", "never" ]
  }
}
